package easy;
public class FibonacciNumber_recursive {
	
	 public static int fib(int n) {
		 if (n == 0) {
			 return 0;
		 }

		 if (n >= 1) {
			 n = fib(n-1)+ fib(n-2);
		 }
		return n;
	 }
	 
	 public static void main(String [] args) {
		 int n = 5;
		 System.out.println(FibonacciNumber_recursive.fib(n));
	 }
}
